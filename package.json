{
	"name": "nm-tool",
	"displayName": "Nm tool",
	"description": "Show Nm tool result in VSCode",
	"version": "0.0.1",
	"publisher": "wibren-wiersma",
	"repository": "https://github.com/XibrenX/nm-tool",
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Other"
	],
	"extensionKind": [
		"workspace"
	],
	"activationEvents": [
		"onLanguage:cpp",
		"onLanguage:c"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "nm-tool.updateNmStore",
				"title": "Nm tool: Run nm",
				"icon": "$(refresh)"
			}
		],
		"viewsContainers": {
		},
		"views": {
		},
		"viewsWelcome": [
		],
		"menus": {
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^16.18.34",
		"@types/vscode": "^1.73.0",
		"@typescript-eslint/eslint-plugin": "^6.7.0",
		"@typescript-eslint/parser": "^6.7.0",
		"eslint": "^8.26.0",
		"typescript": "^5.3.2"
	}
}
